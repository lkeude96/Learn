import java.util.ArrayList;


public class Main {

	public static void main(String[] args) {
		ArrayList<String> list = new ArrayList<String>();
		
		list.add("California");
		list.add("Oregon");
		list.add("Washington");
		
		System.out.println(list);
		
		list.add("Alaska");
		System.out.println(list);
		
		list.remove(0);
		System.out.println(list);
	}

}
